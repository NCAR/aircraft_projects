# /RAF/2006/503/LRT/UNALTERED/RF04.20060314.180400_031300.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF05.20060325.151000_002600.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF06.20060402.143900_233500.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF07A.20060406.164100_230600.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF07B.20060407.012600_030600.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF08.20060408.120500_210000.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF09.20060415.175800_024900.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF10.20060416.191200_035800.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF11.20060421.130200_195200.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/RF12.20060426.125500_214600.PNI.nc \
# /RAF/2006/503/LRT/UNALTERED/TF04.20060413.135500_224800.PNI.nc \
#)
#
echo "#  $#BITFILES job(s) submitted on `date`"
echo "#   Remove READ passwords from files in bitfile list."
echo "#  For the 13 possible failure codes, see the mspasswd man page."
#
foreach BITFILE ($BITFILES)
  echo \
  mspasswd -rpwd bishop -wpwd RAFDMG -newr OFF $BITFILE
  set result = $status
  if ($result == 0) then
    echo "#  $BITFILE -- OK -- `date`"
  else
    echo "#  $BITFILE  -- Failed with code $result  -- `date`"
  endif
end
#
# Done
echo "#   Completed on `date`"
exit
