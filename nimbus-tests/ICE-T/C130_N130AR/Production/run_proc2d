process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110711_141539_rf04.2d -o ${DAT}/ICE-Trf04.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110712_132239_rf05.2d -o ${DAT}/ICE-Trf05.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110715_115510_rf06.2d -o ${DAT}/ICE-Trf06.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110717_131603_rf07.2d -o ${DAT}/ICE-Trf07.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110722_104000_rf08.2d -o ${DAT}/ICE-Trf08.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110723_122706_rf09.2d -o ${DAT}/ICE-Trf09.nc -xsize -allin
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110724_133904_rf10.2d -o ${DAT}/ICE-Trf10.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110727_114552_rf11.2d -o ${DAT}/ICE-Trf11.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110728_122051_rf12.2d -o ${DAT}/ICE-Trf12.nc
process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110730_121826_rf13.2d -o ${DAT}/ICE-Trf13.nc

process2d ${RAW_DATA_DIR}/ICE-T/PMS2D/20110629_130003_ff02.2d -o ${DAT}/ICE-Tff02.nc
