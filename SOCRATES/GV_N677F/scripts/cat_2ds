#!/bin/sh

# Script for SOCRATES to combine multiple .2DS files per flight into a
# single file.

cd ${RAW_DATA_DIR}/SOCRATES/2DS

mkdir TF01
cat raw/base180102124417.2DS > TF01/base180102124417.2DS

mkdir TF02
cat raw/base180104164600.2DS raw/base180104190727.2DS raw/base180104191121.2DS > TF02/base180104164600.2DS

mkdir RF01
cat raw/base180115223257.2DS raw/base180116000000.2DS raw/base180116005453.2DS raw/base180116024708.2DS raw/base180116025943.2DS > RF01/base180115223257.2DS

mkdir RF02
cat raw/base180119005845.2DS raw/base180119005921.2DS raw/base180119061701.2DS > RF02/base180119005845.2DS

mkdir RF03
cat raw/base180122204629.2DS raw/base180122223145.2DS raw/base180122233922.2DS raw/base180123000000.2DS raw/base180123012031.2DS raw/base180123015341.2DS raw/base180123020626.2DS raw/base180123032954.2DS > RF03/base180122204629.2DS

mkdir RF04
cat raw/base180123230729.2DS raw/base180124000000.2DS raw/base180124004146.2DS raw/base180124034742.2DS raw/base180124042204.2DS raw/base180124043437.2DS raw/base180124044710.2DS > RF04/base180123230729.2DS

mkdir RF05
cat raw/base180125224803.2DS raw/base180126000000.2DS > RF05/base180125224803.2DS

mkdir RF06
cat raw/base180128225745.2DS raw/base180128231736.2DS raw/base180129000000.2DS raw/base180129053600.2DS > RF06/base180128225745.2DS

mkdir RF07
cat raw/base180131005359.2DS raw/base180131054655.2DS > RF07/base180131005359.2DS

mkdir RF08
cat raw/base180203225716.2DS raw/base180204000000.2DS raw/base180204031958.2DS raw/base180204033231.2DS raw/base180204034503.2DS raw/base180204035736.2DS raw/base180204041009.2DS raw/base180204042243.2DS raw/base180204043959.2DS raw/base180204054212.2DS raw/base180204060451.2DS raw/base180204061725.2DS > RF08/base180203225716.2DS

mkdir RF09
cat raw/base180204224553.2DS raw/base180205000000.2DS raw/base180205043614.2DS > RF09/base180204224553.2DS

mkdir RF10
cat raw/base180207205636.2DS raw/base180208000000.2DS raw/base180208041440.2DS raw/base180208042840.2DS > RF10/base180207205636.2DS

mkdir RF11
cat raw/base180217014433.2DS raw/base180217045838.2DS raw/base180217051144.2DS > RF11/base180217014433.2DS

mkdir RF12
cat raw/base180217234840.2DS raw/base180218000000.2DS raw/base180218040451.2DS raw/base180218043950.2DS raw/base180218045222.2DS raw/base180218050455.2DS raw/base180218051818.2DS raw/base180218053108.2DS raw/base180218054340.2DS raw/base180218055612.2DS raw/base180218060844.2DS raw/base180218062116.2DS raw/base180218063348.2DS raw/base180218064620.2DS raw/base180218065852.2DS raw/base180218071124.2DS raw/base180218072356.2DS raw/base180218073628.2DS > RF12/base180217234840.2DS

mkdir RF13
cat raw/base180219230358.2DS raw/base180220000000.2DS raw/base180220032348.2DS raw/base180220051759.2DS > RF13/base180219230358.2DS

mkdir RF14
cat raw/base180221224215.2DS raw/base180222000000.2DS > RF14/base180221224215.2DS

mkdir RF15
cat raw/base180224021121.2DS > RF15/base180224021121.2DS

# There are no 2DS files for FF08. Instrument was not turned on.

mkdir FF09
cat raw/base180227214543.2DS raw/base180227222439.2DS raw/base180227223711.2DS raw/base180227224944.2DS raw/base180227232722.2DS raw/base180228000000.2DS raw/base180228010111.2DS raw/base180228022222.2DS raw/base180228024033.2DS > FF09/base180227214543.2DS

mkdir FF10
cat raw/base180301183046.2DS raw/base180301183123.2DS raw/base180301192351.2DS raw/base180301193625.2DS raw/base180301194856.2DS raw/base180301200128.2DS raw/base180301212100.2DS raw/base180302000000.2DS raw/base180302010229.2DS > FF10/base180301183046.2DS
