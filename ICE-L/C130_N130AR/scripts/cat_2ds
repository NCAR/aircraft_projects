#!/bin/sh

# Script for ICE-L to combine multiple .2DS files per flight into a single
# file.

cd ${RAW_DATA_DIR}/ICE-L/2DS

cd 20071029
cat base071029194046.2DS  base071029201442.2DS  base071029201851.2DS > ../merged/base071029194046.2DS
cd ..

cd 20071105
cat base071105203516.2DS  base071105204658.2DS > ../merged/base071105203516.2DS
cd ..

cd 20071107
cat base071107162315.2DS  base071107164442.2DS  base071107181323.2DS > ../merged/base071107162315.2DS
cd ..

cd 20071113 
cat base_just_a_few_images_base071113150633.2DS base_just2images_base071113152755.2DS base071113180254.2DS > ../merged/base071113150633.2DS
cd ..

cd 20071116
cat base071116174608.2DS  base071116181353.2DS > ../merged/base071116174608.2DS
cd ../

cd 20071118
cat base071118194006.2DS base071118194647.2DS base071118202619.2DS base071118203501.2DS base071118203957.2DS base071118205033.2DS base071118205543.2DS base071118211210.2DS base071118214532.2DS base071118215533.2DS > ../merged/base071118194006.2DS
cd ../

cd 20071121
cat base071121001619.2DS base071121011054.2DS base071121020402.2DS base071121022549.2DS base071121024529.2DS base071121035318.2DS base071121044742.2DS base071121062054.2DS base071121064050.2DS > ../merged/base071121001619.2DS
cd ../

cd 20071204
cat base071205004120.2DS  base071205010006.2DS  base071205010241.2DS > ../merged/base071205004120.2DS
cd ../
