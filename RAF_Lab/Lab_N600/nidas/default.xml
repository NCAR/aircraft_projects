    <serialSensor ID="Mensor_6100" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
	<!-- Turn off internal filtering on the Mensor:
		#1FL ?	query the value (factory default is 90%)
		#1FL 0	set to 0
		#1FL ?
		#1SAVE 	save to EEPROM
	-->
        <sample id="1" scanfFormat="%*1d%f"/>
        <message separator="\n" position="end" length="0"/>
        <prompt string="#1?\r" rate="50"/>
    </serialSensor>

    <serialSensor ID="Mensor_6101" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
	<!-- Turn off internal filtering on the Mensor:
		#1FL ?	query the value (factory default is 90%)
		#1FL 0	set to 0
		#1FL ?
		#1SAVE 	save to EEPROM
	-->
        <sample id="1" scanfFormat="%*1d%f"/>
        <message separator="\n" position="end" length="0"/>
        <prompt string="#1?\r" rate="100"/>
    </serialSensor>

    <serialSensor ID="Paro_DigiQuartz_1000" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
	<!-- initialization commands for the Paro:
	  unprompted mode, set rate with PI (and in sample tag)
	  	If we set MD=2, don't need P4 init string
	  *9900BR=57600
	  *0100EW*0100MD=2	turn off  Model 715 output, unprompted
	  *0100EW*0100UN=2	units=millibars
	  *0100EW*0100PI=10	rate=50
	-->

        <sample id="1" scanfFormat="*%*2d%*2d%f" rate="50">
	</sample>
        <message separator="\n" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="Water_CN_Counter" class="DSMSerialSensor"
        baud="115200" parity="none" databits="8" stopbits="1"
        init_string="SM,4,1\r">
        <sample id="1" rate="10" scanfFormat="D,%f,%f,%*f,%f,%f,%f,%f,%f">
	    <variable name="MCN" units="none" longname="WCN Current Operating Mode"/>
	    <variable name="SFLCN" units="none" longname="WCN Status Flags"/>
	    <!--variable name="CONCN" units="#/cm3" longname="WCN Aggregated Concentration"/-->
	    <variable name="ETCN" units="s" longname="WCN Sample Time (Elapsed Time)"/>
	    <variable name="LTCN" units="s" longname="WCN Live Time"/>
	    <variable name="CNTS" units="count" longname="WCN Accumulated Particle Counts" count="true"/>
	    <variable name="PCN" units="hPa" longname="WCN Absolute Pressure"/>
	    <variable name="RPCN" units="none" longname="WCN Raw Photometric Value"/>
        </sample>
        <sample id="2" rate="10" scanfFormat="S,%f,%f,%f,%f,%f,%f,%f">
	    <variable name="FCNC" units="cm3/min" longname="WCN Sample Flow Rate">
                 <linear slope="0.001" intercept="0.0" units="vlpm"/>
            </variable>
	    <variable name="FTCN" units="cm3/min" longname="WCN Total Flow Rate"/>
	    <variable name="FSHCN" units="cm3/min" longname="WCN Sheath Flow Rate"/>
	    <variable name="PACN" units="hPa" longname="WCN Absolute Pressure"/>
	    <variable name="TSCN" units="deg_C" longname="WCN Saturator Temperature"/>
	    <variable name="TGCN" units="deg_C" longname="WCN Growth Tube Temperature"/>
	    <variable name="TOCN" units="deg_C" longname="WCN Optics Temperature"/>
        </sample>
        <message separator="\r" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="TDLH2O" class="DSMSerialSensor"
        baud="115200" parity="none" databits="8" stopbits="1">
