<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
	<name>StreetMaps Overlays</name>
	<Folder>
		<name>Images</name>
		<GroundOverlay>
			<name>IR Sat</name>
			<Icon>
				<href>../images/latest_ir.jpg</href>
				<refreshMode>onInterval</refreshMode>
				<refreshInterval>120</refreshInterval>
			</Icon>
			<LatLonBox>
				<north>35.53</north>
				<south>21.19</south>
				<east>-74.27</east>
				<west>-90.59</west>
			</LatLonBox>
		</GroundOverlay>

		<GroundOverlay>
			<name>NEXRAD</name>
			<Icon>
				<href>http://radar.weather.gov/ridge/Conus/RadarImg/southeast_radaronly.gif</href>
				<refreshMode>onInterval</refreshMode>
				<refreshInterval>120</refreshInterval>
			</Icon>
			<LatLonBox>
				<north>36.928147474567794</north>
				<south>22.551103322319</south>
				<east>-75.144164368167</east>
				<west>-90.24006072802854</west>
			</LatLonBox>
		</GroundOverlay>
	</Folder>


	<!-- THIS IS THE VECTOR FOLDER, ADD ANY VECTOR KMLS (i.e. airport VORS) YOU WANT TO DISPLAY INTO THIS FOLDER	-->
	<Folder>
		<name>Vectors</name>
		<NetworkLink>	
			<name>Lightning</name>
			<Link>	
				<href>../GE/last_hour_lightning.kml</href>
				<refreshMode>onInterval</refreshMode>
				<refreshInterval>60</refreshInterval>
			</Link>
		</NetworkLink>	

		<NetworkLink>	
			<name>Track</name>
			<Link>	
				<href>../GE/real-time.kmz</href>
				<refreshMode>onInterval</refreshMode>
				<refreshInterval>60</refreshInterval>
			</Link>
		</NetworkLink>
	</Folder>
</Document>
</kml>

