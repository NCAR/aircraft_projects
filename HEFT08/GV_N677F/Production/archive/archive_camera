# /bin/csh -f
#
#  Script to run the archcam.508 script for all the available PACDEX flights
#
echo " ${0} started `date`"
#
#  1st wave
archcam.508 -a -f RF01 -p -t
archcam.508 -a -f RF02 -p -t
archcam.508 -a -f RF03 -p -t
archcam.508 -a -f RF04 -p -t
archcam.508 -a -f RF05 -p -t
archcam.508 -a -f RF06 -p -t
#TF01: No images from this flight
#archcam.508 -a -f TF01 -p -t
archcam.508 -a -f TF02 -p -t
archcam.508 -a -f TF03 -p -t

echo " ${0} completed `date`"
exit
