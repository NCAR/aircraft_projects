procf2dc ${RAW_DATA_DIR}/HIPPO-5/PMS2D/20110824_181158_rf07.2d -o ${DAT}/HIPPO-5rf07.nc
procf2dc ${RAW_DATA_DIR}/HIPPO-5/PMS2D/20110827_181943_rf08.2d -o ${DAT}/HIPPO-5rf08.nc
procf2dc ${RAW_DATA_DIR}/HIPPO-5/PMS2D/20110829_201225_rf09.2d -o ${DAT}/HIPPO-5rf09.nc
