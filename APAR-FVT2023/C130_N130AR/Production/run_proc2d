#!/bin/csh

echo $DAT

# files from extract2d

process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/2ds_oap/20231011_164000_rf01.2d -o ${DAT}/APAR-FVT2023rf01.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/2ds_oap/20231018_143600_rf02.2d -o ${DAT}/APAR-FVT2023rf02.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/2ds_oap/20231021_211800_rf04.2d -o ${DAT}/APAR-FVT2023rf03.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/2ds_oap/20231022_174000_rf05.2d -o ${DAT}/APAR-FVT2023rf04.nc

exit

# files from translate2ds

process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/oapfiles/20220402155947_RF01.2d -o ${DAT}/APAR-FVT2023rf01.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/oapfiles/20220407141445_RF02.2d -o ${DAT}/APAR-FVT2023rf02.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/oapfiles/20220413171420_RF03.2d -o ${DAT}/APAR-FVT2023rf03.nc
process2d ${RAW_DATA_DIR}/APAR-FVT2023/2DS/oapfiles/20220415181159_RF04.2d -o ${DAT}/APAR-FVT2023rf04.nc
