	    <variable name="MCN" units="none" longname="WCN Current Operating Mode"/>
	    <variable name="SFLCN" units="none" longname="WCN Status Flags"/>
	    <!--variable name="CONCN" units="#/cm3" longname="WCN Aggregated Concentration"/-->
