            <variable name="LSRTMP" units="ohm" longname="VCSEL Laser Temperature"/>
            <variable name="SETTMP" units="ohm" longname="VCSEL Set Temperature"/>
            <variable name="TCC" units="count" longname="VCSEL Thermoelectric Cooler Current"/>
