            <variable name="CT7" units="none" longname="CFDC Filter"/>
        </sample>
        <message separator="\r\n" position="end" length="0"/>
    </serialSensor>

    <serialSensor ID="CCN" class="DSMSerialSensor"
        baud="57600" parity="none" databits="8" stopbits="1">
        <sample id="1" rate="1" scanfFormat="%f, %f, %f, %f, %f, %f, %f, %f, %f">
